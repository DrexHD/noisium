{
  "schemaVersion": 1,
  "id": "noisium",
  "version": "${version}",
  "name": "Noisium",
  "description": "Optimises worldgen performance for a better gameplay experience.",
  "authors": [
    "Steveplays28"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/noisium",
    "sources": "https://github.com/Steveplays28/noisium",
    "issues": "https://github.com/Steveplays28/noisium/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/noisium/icon.png",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/KbWxgGg"
      }
    },
    "mc-publish": {
      "loaders": [
        "fabric",
        "quilt"
      ],
      "curseforge": -1,
      "modrinth": "-1"
    }
  },
  "environment": "*",
  "accessWidener" : "noisium.accesswidener",
  "entrypoints": {
    "main": [
      "io.github.steveplays28.noisium.Noisium"
    ]
  },
  "mixins": [
    "noisium.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "1.20.x"
  }
}
